@model GilbertMusicStore.Models.SemiAcousticGuitar
@{
	ViewBag.Title = "CreateSemiAcousticGuitar";
}
<h2>Новая полу-акустическая гитара</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm())
{
	@Html.ValidationSummary(true)
	<fieldset>
		<legend>SemiAcousticGuitar</legend>
		@Html.Partial("_GuitarCreateView")

		<div class="editor-label">
			@Html.LabelFor(model => model.BodyTypeId, "BodyType")
		</div>
		<div class="editor-field">
			@Html.DropDownList("BodyTypeId", String.Empty)
			@Html.ValidationMessageFor(model => model.BodyTypeId)
		</div>
		<div class="editor-label">
			@Html.LabelFor(model => model.PreampId)
		</div>
		<div class="editor-field">
			@Html.DropDownList("PreampId", String.Empty)
			@Html.ValidationMessageFor(model => model.PreampId)
		</div>
		<div class="editor-label">
			@Html.LabelFor(model => model.PickupId)
		</div>
		<div class="editor-field">
			@Html.DropDownList("PickupId", String.Empty)
			@Html.ValidationMessageFor(model => model.PickupId)
		</div>
		<p>
			<input type="submit" value="Create" />
		</p>
	</fieldset>
}
<div>
	@Html.ActionLink("Back to List", "Index")
</div>
